<?xml version="1.0"?>
<!--
This file describes the block offsets to center a block in a 
 5x5x5 Cube. needed for converting between classic and new blocks!
-->
<blocks>
<!-- conveyor offsets -->
  <block subtype="ConveyorTube">
    <offset Forward="Forward" Up="Up" x="1" y="0" z="1"/>
    <offset Forward="Forward" Up="Down" x="1" y="0" z="1"/>
    <offset Forward="Left" Up="Backward" x="1" y="1" z="0"/>
    <offset Forward="Down" Up="Right" x="0" y="1" z="1"/>
    <offset Forward="Left" Up="Forward" x="1" y="1" z="0"/>
    <offset Forward="Down" Up="Left" x="0" y="1" z="1"/>
  </block>
  <block subtype="ConveyorTubeCurved">
    <offset Forward="Forward" Up="Up" x="1" y="0" z="1"/>
    <offset Forward="Left" Up="Backward" x="1" y="1" z="0"/>
    <offset Forward="Forward" Up="Down" x="0" y="1" z="1"/>
    <offset Forward="Up" Up="Right" x="0" y="1" z="0"/>
    <offset Forward="Up" Up="Left" x="1" y="1" z="1"/>
    <offset Forward="Left" Up="Forward" x="1" y="0" z="1"/>
  </block>
<!-- functional blocks -->
<!-- armor offsets -->
  <block subtype="LargeBlockArmorSlope2Tip">
    <offset Forward="Left" Up="Forward" x="0" y="0" z="2"/>
    <offset Forward="Right" Up="Down" x="0" y="2" z="0"/>
    <offset Forward="Forward" Up="Left" x="2" y="0" z="0"/>
    <offset Forward="Right" Up="Down" x="0" y="2" z="0"/>
    <offset Forward="Right" Up="Forward" x="0" y="0" z="2"/>
    <offset Forward="Down" Up="Left" x="2" y="0" z="0"/>
    <offset Forward="Down" Up="Forward" x="0" y="0" z="2"/>
    <offset Forward="Backward" Up="Down" x="0" y="2" z="0"/>
  </block>
  <block subtype="LargeBlockArmorCorner2Tip">
    <offset Forward="Forward" Up="Up" x="0" y="0" z="0"/>
  </block>
</blocks>